<?xml version="1.0"?>
<cachesettings title="Cache Configuration Tool"
   contentid="portal_cache_settings" portaltype="CacheTool">
 <field name="cacheConfig" value="zserver"/>
 <field name="domains"/>
 <field name="squidURLs" value=""/>
 <field name="gzip" value="accept-encoding"/>
 <field name="varyHeader" value="Accept-Encoding, Accept-Language"/>
 <headersets title="Headers" contentid="headersets"
    portaltype="HeaderSetFolder">
  <headerset title="Cache in browser for 1 hour"
     contentid="cache-in-browser-1-hour" portaltype="HeaderSet">
   <field name="description"
      value="Cache the page in the client's browser for 1 hour.  Also caches in the proxy for up to 1 hour, and if the template is associated with PageCacheManager, the page may be cached in memory on the server."/>
   <field name="pageCache" value="0"/>
   <field name="lastModified" value="yes"/>
   <field name="etag" value="0"/>
   <field name="enable304s" value="0"/>
   <field name="vary" value="1"/>
   <field name="maxAge" value="3600"/>
   <field name="sMaxAge" value="3600"/>
   <field name="mustRevalidate" value="1"/>
   <field name="proxyRevalidate" value="1"/>
   <field name="noCache" value="0"/>
   <field name="noStore" value="0"/>
   <field name="public" value="1"/>
   <field name="private" value="0"/>
   <field name="noTransform" value="0"/>
   <field name="preCheck" value=""/>
   <field name="postCheck" value=""/>
  </headerset>
  <headerset title="Cache in browser for 24 hours"
     contentid="cache-in-browser-24-hours" portaltype="HeaderSet">
   <field name="description"
      value="Cache the page in the client's browser for 24 hours.  Also caches in the proxy for up to 24 hours, and if the template is associated with PageCacheManager, the page may be cached in memory on the server."/>
   <field name="pageCache" value="0"/>
   <field name="lastModified" value="yes"/>
   <field name="etag" value="0"/>
   <field name="enable304s" value="0"/>
   <field name="vary" value="1"/>
   <field name="maxAge" value="86400"/>
   <field name="sMaxAge" value="86400"/>
   <field name="mustRevalidate" value="1"/>
   <field name="proxyRevalidate" value="1"/>
   <field name="noCache" value="0"/>
   <field name="noStore" value="0"/>
   <field name="public" value="1"/>
   <field name="private" value="0"/>
   <field name="noTransform" value="0"/>
   <field name="preCheck" value=""/>
   <field name="postCheck" value=""/>
  </headerset>
  <headerset title="Cache in browser forever"
     contentid="cache-in-browser-forever" portaltype="HeaderSet">
   <field name="description"
      value="Cache the file in the client's browser for 1 year.  Also caches in the proxy for up to 1 year.  No ETag is set so the file will not be cached in memory on the server."/>
   <field name="pageCache" value="0"/>
   <field name="lastModified" value="yes"/>
   <field name="etag" value="0"/>
   <field name="enable304s" value="0"/>
   <field name="vary" value="1"/>
   <field name="maxAge" value="31536000"/>
   <field name="sMaxAge" value="31536000"/>
   <field name="mustRevalidate" value="0"/>
   <field name="proxyRevalidate" value="0"/>
   <field name="noCache" value="0"/>
   <field name="noStore" value="0"/>
   <field name="public" value="1"/>
   <field name="private" value="0"/>
   <field name="noTransform" value="0"/>
   <field name="preCheck" value=""/>
   <field name="postCheck" value=""/>
  </headerset>
  <headerset title="Cache in Memory" contentid="cache-in-memory"
     portaltype="HeaderSet">
   <field name="description"
      value="Page should be cached in memory on the server.  Page should not be cached in a proxy cache but may be conditionally cached in the browser.  The browser should validate the page's ETag before displaying a cached page."/>
   <field name="pageCache" value="1"/>
   <field name="lastModified" value="delete"/>
   <field name="etag" value="1"/>
   <field name="enable304s" value="1"/>
   <field name="vary" value="1"/>
   <field name="maxAge" value=""/>
   <field name="sMaxAge" value=""/>
   <field name="mustRevalidate" value="1"/>
   <field name="proxyRevalidate" value="0"/>
   <field name="noCache" value="0"/>
   <field name="noStore" value="0"/>
   <field name="public" value="0"/>
   <field name="private" value="1"/>
   <field name="noTransform" value="0"/>
   <field name="preCheck" value=""/>
   <field name="postCheck" value=""/>
  </headerset>
  <headerset title="Cache in proxy cache for 1 hour"
     contentid="cache-in-proxy-1-hour" portaltype="HeaderSet">
   <field name="description"
      value="Cache the page in the proxy cache for up to 1 hour.  If the template is associated with PageCacheManager, the page may be cached in memory on the server."/>
   <field name="pageCache" value="0"/>
   <field name="lastModified" value="yes"/>
   <field name="etag" value="0"/>
   <field name="enable304s" value="0"/>
   <field name="vary" value="1"/>
   <field name="maxAge" value=""/>
   <field name="sMaxAge" value="3600"/>
   <field name="mustRevalidate" value="1"/>
   <field name="proxyRevalidate" value="0"/>
   <field name="noCache" value="0"/>
   <field name="noStore" value="0"/>
   <field name="public" value="0"/>
   <field name="private" value="0"/>
   <field name="noTransform" value="0"/>
   <field name="preCheck" value=""/>
   <field name="postCheck" value=""/>
  </headerset>
  <headerset title="Cache in proxy cache for 24 hours"
     contentid="cache-in-proxy-24-hours" portaltype="HeaderSet">
   <field name="description"
      value="Cache the page in the proxy cache for up to 24 hours.  If the template is associated with PageCacheManager, the page may be cached in memory on the server."/>
   <field name="pageCache" value="0"/>
   <field name="lastModified" value="yes"/>
   <field name="etag" value="0"/>
   <field name="enable304s" value="0"/>
   <field name="vary" value="1"/>
   <field name="maxAge" value=""/>
   <field name="sMaxAge" value="86400"/>
   <field name="mustRevalidate" value="1"/>
   <field name="proxyRevalidate" value="1"/>
   <field name="noCache" value="0"/>
   <field name="noStore" value="0"/>
   <field name="public" value="0"/>
   <field name="private" value="0"/>
   <field name="noTransform" value="0"/>
   <field name="preCheck" value=""/>
   <field name="postCheck" value=""/>
  </headerset>
  <headerset title="Cache with ETag" contentid="cache-with-etag"
     portaltype="HeaderSet">
   <field name="description"
      value="Page should not be cached in a proxy cache but may be conditionally cached in the browser.  The browser should validate the page's ETag before displaying a cached page."/>
   <field name="pageCache" value="0"/>
   <field name="lastModified" value="delete"/>
   <field name="etag" value="1"/>
   <field name="enable304s" value="1"/>
   <field name="vary" value="1"/>
   <field name="maxAge" value=""/>
   <field name="sMaxAge" value=""/>
   <field name="mustRevalidate" value="1"/>
   <field name="proxyRevalidate" value="0"/>
   <field name="noCache" value="0"/>
   <field name="noStore" value="0"/>
   <field name="public" value="0"/>
   <field name="private" value="1"/>
   <field name="noTransform" value="0"/>
   <field name="preCheck" value=""/>
   <field name="postCheck" value=""/>
  </headerset>
  <headerset title="Cache file with Last-Modified"
     contentid="cache-with-last-modified" portaltype="HeaderSet">
   <field name="description"
      value="File should not be cached in a proxy cache but may be conditionally cached in the browser.  The browser should validate the file's Last-Modified time before displaying a cached file."/>
   <field name="pageCache" value="0"/>
   <field name="lastModified" value="yes"/>
   <field name="etag" value="0"/>
   <field name="enable304s" value="1"/>
   <field name="vary" value="1"/>
   <field name="maxAge" value=""/>
   <field name="sMaxAge" value=""/>
   <field name="mustRevalidate" value="1"/>
   <field name="proxyRevalidate" value="0"/>
   <field name="noCache" value="0"/>
   <field name="noStore" value="0"/>
   <field name="public" value="0"/>
   <field name="private" value="1"/>
   <field name="noTransform" value="0"/>
   <field name="preCheck" value=""/>
   <field name="postCheck" value=""/>
  </headerset>
  <headerset title="Do not cache" contentid="no-cache" portaltype="HeaderSet">
   <field name="description" value="Page should not be cached."/>
   <field name="pageCache" value="0"/>
   <field name="lastModified" value="yes"/>
   <field name="etag" value="0"/>
   <field name="enable304s" value="0"/>
   <field name="vary" value="1"/>
   <field name="maxAge" value=""/>
   <field name="sMaxAge" value=""/>
   <field name="mustRevalidate" value="1"/>
   <field name="proxyRevalidate" value="0"/>
   <field name="noCache" value="1"/>
   <field name="noStore" value="0"/>
   <field name="public" value="0"/>
   <field name="private" value="1"/>
   <field name="noTransform" value="0"/>
   <field name="preCheck" value=""/>
   <field name="postCheck" value=""/>
  </headerset>
 </headersets>
 <rules title="Rules" contentid="rules" portaltype="RuleFolder">
  <policycacherule title="Files &amp; Images" contentid="downloads"
     portaltype="PolicyHTTPCacheManagerCacheRule">
   <field name="description"
      value="Rule for ATFile and ATImage downloads.  Files that are viewable by Anonymous users are cached in squid; files that are not viewable by Anonymous users are cached only on the browser.  ETags are not useful because these files have no personalization."/>
   <field name="cacheManager" value="CacheSetup_OFSCache"/>
   <field name="types">
    <fieldvalue value="Image"/>
    <fieldvalue value="File"/>
   </field>
   <field name="ids"/>
   <field name="cacheStop"/>
  </policycacherule>
  <policycacherule title="HTTPCache" contentid="httpcache"
     portaltype="PolicyHTTPCacheManagerCacheRule">
   <field name="description"
      value="Rule for content associated with HTTPCache.  This content is cached in the proxy and in the browser.  ETags are not useful because these files have no personalization."/>
   <field name="cacheManager" value="HTTPCache"/>
   <field name="types"/>
   <field name="ids"/>
   <field name="cacheStop"/>
  </policycacherule>
  <policycacherule title="CSS &amp; JS" contentid="resource-registries"
     portaltype="PolicyHTTPCacheManagerCacheRule">
   <field name="description"
      value="Rule for CSS and JS generated by ResourceRegistries.  These files are cached &quot;forever&quot; (1 year) in squid and in browsers.  There is no need to purge these files because when they are changed and saved in portal_css/portal_js, their file names change.  ETags are not useful because these files have no personalization."/>
   <field name="cacheManager" value="CacheSetup_OFSCache"/>
   <field name="types">
    <fieldvalue value="File"/>
   </field>
   <field name="ids"/>
   <field name="cacheStop"/>
  </policycacherule>
 </rules>
</cachesettings>
